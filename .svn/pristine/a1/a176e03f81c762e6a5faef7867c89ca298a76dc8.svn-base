/*right_recommend*/
#center .right_recommend{ width:331px; height:616px; border:solid #CCC 1px; margin-top:10px;}
#center .right_recommend img{margin-top:10px; margin-left:10px; }
#center .right_recommend h3{ margin-top:-17px; margin-left:25px;  }
#center .right_recommend  .guanggao{ height:565px; width:304px; margin-top:15px; margin-left:15px;}

/*footer底部*/

#footer{ height:235px; width:1188px;display: block;  height: 0;  clear: both;  }
#footer .cooperation a img{height:40px; width:123px; margin-left:10px; margin-right:10px;  }
#footer .cooperation img.hezuo{padding-top:10px;}
#footer a{ margin:0  10px;color:#000;}
#footer p{ margin-top:21px; text-align:center;}
#footer a:hover{ color:#999 }
